# News Plugin - Environment Variables Example
# Copy this file to .env.local for local development

# Gun.js P2P Relay Servers
# Leave empty for localStorage-only mode (recommended - no network errors)
# Add active Gun.js relay servers for true P2P sync
VITE_GUN_RELAY_1=
VITE_GUN_RELAY_2=
VITE_GUN_RELAY_LOCAL=

# Plugin Configuration
VITE_PLUGIN_NAME=news-plugin
VITE_PLUGIN_VERSION=2.0.0

# Development Settings
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PERFORMANCE_MONITORING=false
